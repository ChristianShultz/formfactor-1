<?xml version="1.0"?>
<FakeDataIni>

  <version>0</version>

  <stateProps>
    <sourceMasses>0.1 0.2 0.3</sourceMasses>
    <sourceVarO>0.1</sourceVarO>
    <sourceUpdateCovariance>true</sourceUpdateCovariance> <!-- really correlation oops-->
    <sourceUpdateVariance>true</sourceUpdateVariance>
    <sinkMasses>0.1 0.2 0.3</sinkMasses>
    <sinkVarO>0.1</sinkVarO>
    <sinkUpdateCovariance>true</sinkUpdateCovariance>
