    <right_target>0</right_target>

    <!-- these control the attributes of the overlaps -->
    <sameOp>false</sameOp>
    <updateCovariance>true</updateCovariance>
    <updateVariance>true</updateVariance>
    <varianceOrder>0.1</varianceOrder>
  </matElemProps>

  <timeProps>
    <tsource>0</tsource>
    <tsink>25</tsink>
  </timeProps>

  <dataProps>
     <ncfg>500</ncfg>

    <momenta>
      <elem>
	<momSource>0 0 1</momSource>
	<momSink>0 0 1</momSink>
      </elem>
      <elem>
	<momSource>0 1 0</momSource>
	<momSink>0 1 0</momSink>
      </elem>
      <elem>
	<momSource>1 0 0</momSource>
	<momSink>1 0 0</momSink>
      </elem>
      <elem>
	<momSource>0 0 1</momSource>
	<momSink>0 1 0</momSink>
      </elem>
      <elem>
	<momSource>0 0 1</momSource>
	<momSink>1 0 0</momSink>
      </elem>
    </momenta>
